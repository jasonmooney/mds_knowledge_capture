# Environment variables for MDS Knowledge Capture Agent

# LangSmith Configuration (for monitoring and tracing)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=mds_knowledge_capture
LANGSMITH_TRACING=true

# OpenAI API Key (if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# ChromaDB Configuration
CHROMA_HOST=localhost
CHROMA_PORT=8000

# Agent Configuration
AGENT_SCHEDULE_ENABLED=true
AGENT_SCHEDULE_INTERVAL=weekly
AGENT_LOG_LEVEL=INFO

# Download Configuration
MAX_CONCURRENT_DOWNLOADS=5
DOWNLOAD_TIMEOUT=300
USER_AGENT=MDS-Knowledge-Capture-Agent/1.0